                if ($Alpha){TopUv += vec2( -#UVOffset.r, #UVOffset.g);}
                float angleRads = radians( #Angle.g + $YAngle);
                float cosAngle = cos( angleRads);
                float sinAngle = sin( angleRads);
                TopUv = mat2( cosAngle, -sinAngle, sinAngle, cosAngle ) * TopUv;
                TopUv *= vec2( $Repeat);
