    #else
    tempTex = managedTexture( $Image  ,TopUv.xy  );
    #endif

    tempTex_AlphaMix = mix(tempTex,vec4(tempTex.rgb,stencil*tempTex.a),AlphaMix);

    result = Comp_Default( result,tempTex_AlphaMix, resC);
