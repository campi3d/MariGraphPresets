    resC = mriCurveLookup($FO,resC*$ExpandY);
    resD = mriCurveLookup($FO,resD*$ExpandMinY);
    resE = mriCurveLookup($FO,resE*$ExpandZ);
    resF = mriCurveLookup($FO,resF*$ExpandMinZ);

