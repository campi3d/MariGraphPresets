#if defined MRI_SHADER_FRAGMENT || defined MRI_SHADER_TESS_EVALUATION
    // distance in texture space [0,1] between adjacent pixels at this mip level
    float Offset = exp2(_State.MipLevel)/u_MriMaxSupportedTextureSize.x;

    float BumpWeight = $BumpWeight/Offset;


#if $BumpMode
    // fudge factor
    BumpWeight *= 1.0/(30.0 * 16.0);

    // Bump mapping version A
    REVAL(#Height,-Offset,0.0);
    float l=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    REVAL(#Height,Offset,0.0);
    float r=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    REVAL(#Height,0.0,-Offset);
    float b=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    REVAL(#Height,0.0,Offset);
    float t=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    vec2 heightDelta = vec2( l - r, b - t);
#else
    // fudge factor
    BumpWeight *= 2.0/(30.0 * 16.0);

    // Bump mapping version A
    float c=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    REVAL(#Height,Offset,0.0);
    float r=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    REVAL(#Height,0.0,Offset);
    float t=(bool( #Height_INVERTED) ? (1.0-#Height.r) : #Height.r)*BumpWeight;

    vec2 heightDelta = vec2( c - r, c - t);
#endif

    Output.rgb = normalize( vec3(heightDelta,1.0) ) * vec3(0.5,-0.5,0.5) + vec3(0.5); 
    Output.a = 1.0; 
#endif
