          float cosAngle = cos( angleRads);
          float sinAngle = sin( angleRads);
          RightUv = mat2( cosAngle, -sinAngle, sinAngle, cosAngle ) * RightUv;
          RightUv *= vec2( $Repeat);
          if ($Alpha == false){RightUv += vec2( -#UVOffset.r, #UVOffset.g);}
          RightUv += vec2(0.5, 0.5);
      }
    }



    vec4 result;
    float AlphaMix = float($Alpha);
    float stencil = 1.0;
    vec4 tempTex;

    // tempTex is used to write the results of each directional projection into a variable
    // tempTex_AlphaMix is stripping out non 0-1 areas if Stencil is on

    // Mari 3.3 changes texture handling
    #if !defined GNZ_TEXTUREMANAGER_GLSLH
    tempTex = texture( $Image, RightUv.xy);
