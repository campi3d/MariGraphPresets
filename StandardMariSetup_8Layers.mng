        {
            float Scale = $DisplacementRange * $DisplacementScale;
            if( 0.0 != Scale)
            {
                // Comment to force evalshader

                // Bump mapping version A
                float Height00 = #Displacement.r;

#if $PerturbNormals
                //float Offset = 1.0/u_MriMaxSupportedTextureSize.x;
                float Offset = 1.0/(32.0 * 1024.0);

                REVAL(#Displacement,+Offset,0.0);
                float Height10 = #Displacement.r;

                REVAL(#Displacement,0.0,+Offset);
                float Height01 = #Displacement.r;

                float HeightX0 = Height10 - Height00;
                float Height0X = Height01 - Height00;

                vec3 SurfaceTangent =   normalize( vec3( Offset, 0.0, (Scale * HeightX0) ));
                vec3 SurfaceBitangent = normalize( vec3( 0.0, Offset, (Scale * -Height0X) ));
                vec3 SurfaceNormal = normalize( cross( SurfaceTangent, SurfaceBitangent));

                mat3 RotMat = mat3(
                    _State.Tangent,
                    _State.Bitangent,
                    _State.Normal
                );
#endif

                _State.Position = _State.Position + (Scale * (Height00 - $DisplacementBias)) * _State.Normal;
#if $PerturbNormals
                _State.Normal = normalize( RotMat * SurfaceNormal);
                _State.Tangent = normalize( RotMat * SurfaceTangent);
                _State.Bitangent = normalize( RotMat * SurfaceBitangent);
#endif

                // Force Displacement to evaluate
                _State.Height = 1.0;
            }
        }
