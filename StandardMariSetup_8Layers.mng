    // Mari 3.3 changes texture handling
    #if !defined GNZ_TEXTUREMANAGER_GLSLH
    tempTex = texture( $Image,FrontUv.xy);
    #else
    tempTex = managedTexture( $Image,FrontUv.xy);
    #endif

        if (
                FrontUv.x > (1.0)  ||
                FrontUv.x &lt; (0.0)  ||
                FrontUv.y &lt; (0.0) ||
                FrontUv.y > (1.0)
            )

            {stencil = 0.0;}

    tempTex_AlphaMix = mix(tempTex,vec4(tempTex.rgb,stencil*tempTex.a),AlphaMix);

    result = Comp_Default( result, tempTex_AlphaMix, resE);
    result = Comp_Default( result, tempTex_AlphaMix, resF);

    Output = result;
    Output.a = _res * Output.a;
    Output = vec4( mix(Output.r,1.0-Output.r,float($Inv)),
                   mix(Output.g,1.0-Output.g,float($Inv)),
                   mix(Output.b,1.0-Output.b,float($Inv)),
                   Output.a
                  );

    
